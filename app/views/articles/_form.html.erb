<%= form_with model: article do |form| %>
  


    <%= form.label :avatar %><br>
    <%= form.file_field :avatar %><br>



    <%= form.label :title %><br>
    <%= form.text_field :title %><br>
    

  
    <%= form.label :body %><br>
    <%= form.text_area :body %><br>
    
    

  <div>
    <%= form.submit %>
  </div>
<% end %>
